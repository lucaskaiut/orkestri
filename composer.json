{
    "name": "lucaskaiut/orkestri",
    "description": "Opinionated architectural scaffold generator for Laravel.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "LucasKaiut\\Orkestri\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucasKaiut\\Orkestri\\OrkestriServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
